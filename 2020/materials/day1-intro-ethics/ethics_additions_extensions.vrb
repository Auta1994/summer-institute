SELECT dropoff_latitude, dropoff_longitude
FROM tripData
WHERE pickup_lat > 40.767249 AND pickup_lat < 40.768
  AND pickup_lon > -73.996782 AND pickup_lon < -73.995538
  AND HOUR(pickup_datetime) < 6
  AND trip_distance > 5;
\end{lstlisting}
